 114:				; jgh: macro syntax changed for ZMAC and MAXAM
 115:				;	can't use opcodes as labels
 116:				;	ZMAC allows &nn as hex, so & removed from local labels
 117:				;
 118:				tstr	macro	insn1,insn2,insn3,insn4,memop,riy,rix,rhl,rde,rbc,flags,acc,rsp,?lab
 119:				?lab:	db	insn1,insn2,insn3,insn4
 120:					dw	memop,riy,rix,rhl,rde,rbc
 121:					db	flags
 122:					db	acc
 123:					dw	rsp
 124:					if	$-?lab ne 20
 125:					error	'missing parameter'
 126:					endif
 127:					endm
 128:				
 129:				tmsg	macro	msg,?lab
 130:				?lab:	db	msg
 131:					if	$ ge ?lab+31
 132:					error	'message too long'
 133:					else
 134:				;	ds	?lab+30-$,'.'	; jgh: ZMAC/MAXAM don't have char parameter
 135:					endif
 136:					db	'$'
 137:					endm
