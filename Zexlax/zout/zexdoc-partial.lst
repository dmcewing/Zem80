 117:				; jgh: macro syntax changed for ZMAC and MAXAM
 118:				;	can't use opcodes as labels
 119:				;	ZMAC allows &nn as hex, so & removed from local labels
 120:				;
 121:				tstr	macro	insn1,insn2,insn3,insn4,memop,riy,rix,rhl,rde,rbc,flags,acc,rsp,?lab
 122:				?lab:	db	insn1,insn2,insn3,insn4
 123:					dw	memop,riy,rix,rhl,rde,rbc
 124:					db	flags
 125:					db	acc
 126:					dw	rsp
 127:					if	$-?lab ne 20
 128:					error	'missing parameter'
 129:					endif
 130:					endm
 131:				
 132:				tmsg	macro	msg,?lab
 133:				?lab:	db	msg
 134:					if	$ ge ?lab+31
 135:					error	'message too long'
 136:					else
 137:				;	ds	?lab+30-$,'.'	; jgh: ZMAC/MAXAM don't have char parameter
 138:					endif
 139:					db	'$'
 140:					endm
