 187:				; jgh: macro syntax changed for ZMAC and MAXAM
 188:				;	can't use opcodes as labels
 189:				;	ZMAC allows &nn as hex, so & removed from local labels
 190:				;
 191:				tstr	macro	insn1,insn2,insn3,insn4,memop,riy,rix,rhl,rde,rbc,flags,acc,rsp,?lab
 192:				?lab:	db	insn1,insn2,insn3,insn4
 193:					dw	memop,riy,rix,rhl,rde,rbc
 194:					db	flags
 195:					db	acc
 196:					dw	rsp
 197:					if	$-?lab ne 20
 198:					error	'missing parameter'
 199:					endif
 200:					endm
 201:				
 202:				tmsg	macro	msg,?lab
 203:				?lab:	db	msg
 204:					if	$ ge ?lab+31
 205:					error	'message too long'
 206:					else
 207:				;	ds	?lab+30-$,'.'	; jgh: ZMAC/MAXAM don't have char parameter
 208:					endif
 209:					db	'$'
 210:					endm
