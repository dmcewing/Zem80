 116:				; jgh: macro syntax changed for ZMAC and MAXAM
 117:				;	can't use opcodes as labels
 118:				;	ZMAC allows &nn as hex, so & removed from local labels
 119:				;
 120:				tstr	macro	insn1,insn2,insn3,insn4,memop,riy,rix,rhl,rde,rbc,flags,acc,rsp,?lab
 121:				?lab:	db	insn1,insn2,insn3,insn4
 122:					dw	memop,riy,rix,rhl,rde,rbc
 123:					db	flags
 124:					db	acc
 125:					dw	rsp
 126:					if	$-?lab ne 20
 127:					error	'missing parameter'
 128:					endif
 129:					endm
 130:				
 131:				tmsg	macro	msg,?lab
 132:				?lab:	db	msg
 133:					if	$ ge ?lab+31
 134:					error	'message too long'
 135:					else
 136:				;	ds	?lab+30-$,'.'	; jgh: ZMAC/MAXAM don't have char parameter
 137:					endif
 138:					db	'$'
 139:					endm
