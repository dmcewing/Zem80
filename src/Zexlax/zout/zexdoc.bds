01be 01be s 	db	flags
01be 01be s 	db	acc
01be 01be s 	dw	rsp
01be 01be s 	if	$-?lab ne 20
01be 01be s 	error	'missing parameter'
01be 01be s 	endif
01be 01be s 	endm
01be 01be s 
