01be 01be s ; jgh: macro syntax changed for ZMAC and MAXAM
01be 01be s ;	can't use opcodes as labels
01be 01be s ;	ZMAC allows &nn as hex, so & removed from local labels
01be 01be s ;
01be 01be s tstr	macro	insn1,insn2,insn3,insn4,memop,riy,rix,rhl,rde,rbc,flags,acc,rsp,?lab
